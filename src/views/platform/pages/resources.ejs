  <script>
function myFunction() {
    var input, filter, cards, cardContainer, cardContent, i;
    input = document.getElementById("myFilter");
    filter = input.value.toUpperCase();
    cardContainer = document.getElementById("myItems");
    cards = cardContainer.getElementsByClassName("card");
    
    for (i = 0; i < cards.length; i++) {
        cardContent = cards[i].querySelector(".card-body");
        
        // Concatenate all relevant text content within the card
        var cardText = cardContent.textContent || cardContent.innerText;
        
        if (cardText.toUpperCase().indexOf(filter) > -1) {
            cards[i].style.display = "";
        } else {
            cards[i].style.display = "none";
        }
    }
}

